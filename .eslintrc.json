{
  "parser": "babel-eslint",
	"extends": "airbnb",
	"rules": {
		"react/jsx-filename-extension": 0,
    "func-names": 0,
    "import/prefer-default-export": 0,
    "react/prop-types": 0,
    "global-require": 0,
    "no-unused-vars": 0,
    "no-useless-escape": 0,
    "dot-notation": 0,
    "jsx-quotes": 0,
    "no-restricted-syntax": 0,
    "no-param-reassign": 0,
    "no-case-declarations": 0
	}
}
